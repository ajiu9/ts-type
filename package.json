{
  "name": "ts-type",
  "type": "module",
  "version": "0.0.0",
  "packageManager": "pnpm@9.0.2",
  "description": "Commonly used typescript types",
  "author": "Ajiu9 <615944323@qq.com>",
  "license": "MIT",
  "homepage": "https://github.com/ajiu9/ts-type#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ajiu9/ts-type.git"
  },
  "bugs": "https://github.com/ajiu9/ts-type/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm build",
    "dev": "pnpm dev",
    "lint": "eslint .",
    "prepublishOnly": "pnpm run build",
    "release": "bumpp && npm publish",
    "start": "pnpm src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "devDependencies": {
    "@ajiu9/eslint-config": "^2.5.2",
    "@types/node": "^20.17.15",
    "bumpp": "^9.10.2",
    "eslint": "^9.18.0",
    "lint-staged": "^15.4.1",
    "pnpm": "^9.15.4",
    "rimraf": "^5.0.10",
    "simple-git-hooks": "^2.11.1",
    "typescript": "^5.7.3",
    "vite": "^5.4.14",
    "vitest": "^1.6.0"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
